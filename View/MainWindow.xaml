<Window x:Class="DigitalWellBeingApp.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:DigitalWellBeingApp.ViewModels"
        Title="Digital Well-Being"
        Height="575"
        Width="806">
    <Grid Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" Opacity="0.4">
            <Border.Background>
                <ImageBrush ImageSource="/View/coding.jpg"/>
            </Border.Background>
        </Border>
        <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1" Opacity="0.4">
            <Border.Background>
                <ImageBrush ImageSource="/View/league.jpg" Stretch="UniformToFill"/>
            </Border.Background>
        </Border>
    </Grid>
</Window>
